{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/(certificaciones)/estratificacion/page.tsx", "src/certificados/components/SeleccionPredio.jsx", "src/certificados/components/GenerarPDF.jsx", "src/utils/certificaciones/generarPDFEstratificacion.js", "src/mantenimiento/components/TablaDesbloqueos.jsx", "src/app/(certificaciones)/mis-certificaciones/page.jsx", "src/certificados/components/TablaPredio.tsx"],
  "exclude": ["node_modules"]
}
